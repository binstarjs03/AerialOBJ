<Window x:Class="binstarjs03.CubeOBJ.WpfApp.UIElements.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:binstarjs03.CubeOBJ.WpfApp.UIElements.Windows"
        xmlns:controls="clr-namespace:binstarjs03.CubeOBJ.WpfApp.UIElements.Controls"
        mc:Ignorable="d"
        Title="MainWindow" 
        
        Width="800" Height="600"
        MinWidth="450" MinHeight="300"
        d:Width="450" d:Height="300">
    <Grid>
        <DockPanel>
            <Menu IsMainMenu="True" DockPanel.Dock="Top">
                <MenuItem Header="File"/>
                <MenuItem Header="View"/>
                <MenuItem Header="Help"/>
            </Menu>
            <controls:ViewportControl/>
        </DockPanel>
    </Grid>
</Window>
